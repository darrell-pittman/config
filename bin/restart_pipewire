#!/bin/sh
echo "Terminating PipeWire processes ..."
pkill -u "${USER}" -x pipewire\(-pulse\)?\|wireplumber\|mpris-proxy 1>/dev/null 2>&1
pidwait -u "${USER}" -x pipewire\(-pulse\)?\|wireplumber\|mpris-proxy
echo "PipeWire terminated."

pipewire &

pipewire-pulse &

sleep 1

wireplumber &

sleep 1

mpris-proxy &



